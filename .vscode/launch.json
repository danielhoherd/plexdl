// https://json5.org
{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: plexdl",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/plexdl/cli.py",
            "console": "integratedTerminal",
            // Add PLEXDL_USER, PLEXDL_PASS, PLEXDL_TITLE to dev.env
            // bash: source dev.env && export "${!PLEXDL_@}"
            "envFile": "dev.env",
        }
    ]
}